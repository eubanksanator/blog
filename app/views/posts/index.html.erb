

<%= notice %>

<% @posts.each do |post| %>
  <div class="panel panel-default col-md-offset-2 col-md-8">
    <div class="panel-heading">
      <h3 class="panel-title"><%= link_to post.title, post %> <small style="color:orange"><%= post.created_at.strftime("%B, %d, %Y") %></small></h3>
    </div>
    <div class="panel-body">
      <%= post.body %>
    </div>
  </div>
  <% end %>


