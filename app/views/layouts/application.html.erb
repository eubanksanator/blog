<!DOCTYPE html>
<html>
<head>
  <title>Blog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <link href='http://fonts.googleapis.com/css?family=Poiret+One' rel='stylesheet' type='text/css'>
  <link href='http://fonts.googleapis.com/css?family=Fugaz+One' rel='stylesheet' type='text/css'>

  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

</head>
<body>
  <div class="container">

    <div class="nav">
      <div class="row">
        <div class="text-center">
          <div class="page-header">
            <h1 class="title-header"><%= link_to 'Travis Eubanks', root_path %></h1>

            <div class="buttons">
              <button class="button"><%= link_to 'All Posts', root_url %></button>
              <button class="button"><%= link_to 'New Post', new_post_url %></button>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="sidebar">
      <ul class="sidebar-links">
        <li><%= link_to "LinkedIn", "#" %></li>
        <li><%= link_to "Twitter", '#' %></li>
        <li><%= link_to "Github", "#" %></li>
      </ul>
    </div>


    <%= yield %>
  </div>


</body>
</html>
